{
    "compilerOptions": {
        "module": "es6",
        "target": "es6",
        "sourceMap": true,
        "fast": "always",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors":true
    },
    "exclude": [
      "node_modules",
      "config/typings"
    ]
}
